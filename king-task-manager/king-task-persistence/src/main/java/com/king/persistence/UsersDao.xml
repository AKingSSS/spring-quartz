<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.king.persistence.UsersDao">

  <insert id="saveUser" parameterType="com.king.domin.User">
    insert into user
    (name, age)
    values
    (#{name},#{age})
  </insert>
</mapper>